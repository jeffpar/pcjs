      "args": [
        "DISK0186.json",
        "t.json"
      ],
      "cwd": "${workspaceFolder}/tmp",
      "stopOnEntry": false,
      "console": "integratedTerminal",
      "outFiles": [
        "${workspaceFolder}/none/**/*.js"
      ]
    },
    {
      "name": "Node: diskimage.js --all --zip",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/tools/diskimage/diskimage.js",
      "args": [
        "--all='/disks/pcsigdisks/pcx86/sw/misc/pcsig/*/DISK*/archive/DISK*.ZIP'",
        "--extract",
        "--expand",
        "--normalize",
        "--overwrite",
        "--verbose",
        "--extdir=%d",
      ],
      "cwd": "${workspaceFolder}",
      "stopOnEntry": false,
      "console": "integratedTerminal",
      "outFiles": [
        "${workspaceFolder}/none/**/*.js"
      ]
    },
    {
      "name": "Node: diskimage.js --all --zip --filter",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/tools/diskimage/diskimage.js",
      "args": [
        "--all='/Volumes/PCSIG_13B/**/*.ZIP'",
        "--filter='00[0-9][0-9]'",
        "--verbose"
      ],
      "cwd": "${workspaceFolder}",
      "stopOnEntry": false,
      "console": "integratedTerminal",
      "outFiles": [
